<?xml version="1.0" encoding="UTF-8"?>
<BeeView xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../schemas/view.xsd"

  name="Users"
  source="Users"
  >
  <BeeColumns>
    <BeeColumn name="Login" expression="Login" />
    <BeeColumn name="Password" expression="Password" />
    <BeeColumn name="FirstName" expression="Person>FirstName" />
    <BeeColumn name="LastName" expression="Person>LastName" />
    <BeeColumn name="Position" expression="Person>Position" />
    <BeeColumn name="Properties" expression="Properties" />
    <BeeColumn name="Company" expression="Person>Company" />
    <BeeColumn name="CompanyName" expression="Person>Company>Name" />
  </BeeColumns>
  <BeeOrder>
    <OrderBy column="Login" />
  </BeeOrder>
</BeeView>
 