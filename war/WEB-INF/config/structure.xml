<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<BeeTables xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../schemas/structure.xsd">

  <BeeTable name="Sequences" idName="SequenceID">
    <BeeFields>
      <BeeField name="Name" type="STRING" precision="30" notNull="true" unique="true" />
      <BeeField name="LastValue" type="LONG" notNull="true" />
    </BeeFields>
  </BeeTable>
  <BeeTable name="Users" idName="UserID">
    <BeeFields>
      <BeeField name="Login" type="STRING" precision="30" notNull="true" unique="true" />
      <BeeField name="Password" type="STRING" precision="64" />
      <BeeField name="FirstName" type="STRING" precision="30" />
      <BeeField name="LastName" type="STRING" precision="30" />
      <BeeField name="Position" type="STRING" precision="50" />
      <BeeField name="Properties" type="STRING" precision="2000" />
    </BeeFields>
  </BeeTable>
  <BeeTable name="Roles" idName="RoleID">
    <BeeFields>
      <BeeField name="Name" type="STRING" precision="30" notNull="true" unique="true" />
    </BeeFields>
  </BeeTable>
  <BeeTable name="UserRoles" idName="UserRoleID">
    <BeeFields>
      <BeeField name="User" type="LONG" notNull="true" relation="Users" cascade="true" />
      <BeeField name="Role" type="LONG" notNull="true" relation="Roles" cascade="true" />
      <BeeKey fields="User, Role" unique="true" />
    </BeeFields>
  </BeeTable>
  <BeeTable name="Countries" idName="CountryID" versionName="LastLock">
    <BeeFields>
      <BeeField name="Name" type="STRING" precision="100" notNull="true" unique="true" />
      <BeeField name="Area" type="DECIMAL" precision="10" scale="2" />
      <BeeField name="SeaPort" type="LONG" relation="Cities" />
      <BeeField name="Neighbour" type="LONG" relation="Countries" />
      <BeeField name="Currency" type="STRING" precision="50" />
    </BeeFields>
  </BeeTable>
  <BeeTable name="Cities" idName="CityID" states="Marked, Unused">
    <BeeFields>
      <BeeField name="Name" type="STRING" precision="100" notNull="true" unique="true" />
      <BeeField name="Population" type="INTEGER" />
      <BeeField name="Country" type="LONG" relation="Countries" />
    </BeeFields>
  </BeeTable>
</BeeTables>
