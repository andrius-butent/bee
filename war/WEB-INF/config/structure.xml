<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<BeeTables>
  <BeeTable name="Countries" idName="CountryID" lockName="LastLock">
    <BeeStructure>
      <BeeField name="Country" type="STRING" precision="100" notNull="true" unique="true" />
      <BeeField name="Area" type="NUMERIC" precision="10" scale="2" />
      <BeeField name="SeaPort" type="LONG" relation="Cities" />
      <BeeField name="Neighbour" type="LONG" relation="Countries" />
      <BeeField name="Currency" type="STRING" precision="50" unique="true" />
    </BeeStructure>
    <BeeExtended>
      <BeeField name="Language" type="STRING" precision="10" />
      <BeeKey name="Language" fields="Language" />
    </BeeExtended>
  </BeeTable>
  <BeeTable name="Cities" idName="CityID">
    <BeeStructure>
      <BeeField name="City" type="STRING" precision="100" notNull="true" unique="true" />
      <BeeField name="Population" type="INTEGER" />
      <BeeField name="Country" type="LONG" relation="Countries" />
    </BeeStructure>
  </BeeTable>
</BeeTables>
