<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<BeeViews xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../schemas/view.xsd">

  <BeeView name="UserRoles" source="UserRoles" readOnly="false">
    <BeeColumns>
      <BeeColumn name="User" expression="User" />
      <BeeColumn name="UserLogin" expression="User-Login" />
      <BeeColumn name="Role" expression="Role" />
      <BeeColumn name="RoleName" expression="Role>Name" />
    </BeeColumns>
    <BeeOrder>
      <OrderBy name="User" />
      <OrderBy name="RoleName" descending="true" />
    </BeeOrder>
  </BeeView>
</BeeViews>
