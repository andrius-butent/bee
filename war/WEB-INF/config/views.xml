<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<BeeViews xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee views.xsd">

  <BeeView name="Nationalities" source="Nationalities" readOnly="false">
    <BeeColumns>
      <BeeColumn name="Name" expression="Name" />
      <BeeColumn name="Percent" expression="Percent" />
      <BeeColumn name="Country" expression="Country" />
      <BeeColumn name="CountryName" expression="Country>Name" />
      <BeeColumn name="CountryCapitalName" expression="Country>Capital>Name" />
      <BeeColumn name="CountryCapitalPopulation" expression="Country>Capital>Population" />
    </BeeColumns>
  </BeeView>
</BeeViews>
