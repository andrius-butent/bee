<?xml version="1.0" encoding="UTF-8"?>
<g:Grid xmlns="http://www.butent.com/column"
        xmlns:g="http://www.butent.com/grid"
        xmlns:ui="http://www.butent.com/ui"
  
  name="ItemRemainders" viewName="ItemRemainders" caption="=tcdRemainders"
  flexGrow="1" flexShrink="1" readOnly="true"
  >
  <g:columns>
    <RelColumn name="Warehouse" />
    <DataColumn name="Remainder" maxWidth="80" />
    <PropColumn name="ResRemainder" type="double" label="=ordResRemainder" module="Orders" />
    <CalcColumn name="FreeRemainder" type="double" label="=ordFreeRemainder" module="Orders">
      <render>
        <ui:expression>(row.Remainder ? row.Remainder : 0 ) - row.ResRemainder
        </ui:expression>
      </render>
    </CalcColumn>
  </g:columns>
</g:Grid>
