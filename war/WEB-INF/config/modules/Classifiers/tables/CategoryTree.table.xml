<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="CategoryTree"
  idName="CategoryID"
  >
  <Fields>
    <String name="Name" precision="60" notNull="true" label="=name" />
    <String name="Name2" precision="60" label="=itemName2" />
    <String name="Name3" precision="60" label="=itemName3" />
    <Relation name="Parent" relation="CategoryTree" label="=category" />

    <Boolean name="Included" label="=included" />

    <Relation name="Dimensions" unique="true" relation="Dimensions" editable="true"
      label="=dimensions" />
  </Fields>

  <Constraints>
    <Unique fields="Parent Name" />
  </Constraints>
</Table>
