<?xml version="1.0" encoding="UTF-8"?>
<View xmlns="http://www.butent.com/view"

      name="VehicleModels" source="VehicleModels" caption="=vehicleModels"
      newRowCaption="=trNewVehicleModel"
>
  <Columns>
    <SimpleColumn name="Brand" />
    <SimpleJoin name="Brand" joinType="INNER">
      <SimpleColumn name="BrandName" />
    </SimpleJoin>

    <SimpleColumn name="Name" />

    <SimpleColumn name="Item"/>
    <SimpleJoin joinType="LEFT" name="Item">
      <SimpleColumn name="Name" alias="ItemName" />
      <SimpleColumn name="Article" />
    </SimpleJoin>
  </Columns>

  <Order>
    <OrderBy column="BrandName" />
    <OrderBy column="Name" />
  </Order>
</View>
