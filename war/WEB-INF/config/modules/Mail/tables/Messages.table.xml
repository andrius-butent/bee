<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="Messages"
  idName="MessageID"
  >
  <Fields>
    <String name="UniqueId" precision="32" unique="true" notNull="true" />
    <DateTime name="Date" notNull="true" />
    <Relation name="Sender" relation="Emails" notNull="true" />
    <String name="Subject" precision="255" />
    <Numeric name="AttachmentCount" precision="2" />
  </Fields>
  <Keys>
    <Key>
      <KeyField>Date</KeyField>
    </Key>
  </Keys>
</Table>
