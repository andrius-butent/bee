<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="Messages"
  idName="MessageID"
  >
  <Fields>
    <String name="UniqueId" precision="100" notNull="true" unique="true" />
    <Relation name="Header" relation="Headers" unique="true" editable="true" />
    <DateTime name="Date" notNull="true" />
    <Relation name="Sender" relation="Addresses" notNull="true" />
    <String name="Subject" precision="100" />
    <Numeric name="Status" precision="1" notNull="true" />
  </Fields>
  <Keys>
    <Key>
      <KeyField>Status</KeyField>
    </Key>
  </Keys>
</Table>
