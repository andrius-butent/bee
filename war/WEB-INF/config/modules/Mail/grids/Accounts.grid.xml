<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Grid xmlns="http://www.butent.com/grid"
  xmlns:ui="http://www.butent.com/ui"

  name="Accounts" viewName="Accounts" caption="Paskyros"
  >

  <columns>
    <DataColumn name="Description" caption="Pavadinimas" />
    <DataColumn name="Main" caption="Pagr." />
    <RelColumn name="Address" caption="El.paštas">
      <renderToken source="Label" suffix=" " />
      <renderToken source="Email" prefix="&lt;" suffix="&gt;" />
    </RelColumn>
    <DataColumn name="StoreLogin" caption="Prisijungimo vardas" />
    <DataColumn name="StorePassword" caption="Slaptažodis" />
    <DataColumn name="StoreType" caption="Serverio tipas">
      <editor type="list">
        <ui:item>POP3</ui:item>
        <ui:item>POP3S</ui:item>
        <ui:item>IMAP</ui:item>
        <ui:item>IMAPS</ui:item>
      </editor>
    </DataColumn>
    <DataColumn name="StoreServer" caption="Serveris" />
    <DataColumn name="StorePort" caption="Prievadas" />

    <DataColumn name="TransportSSL" caption="SMTP SSL" />
    <DataColumn name="TransportServer" caption="SMTP serveris" />
    <DataColumn name="TransportPort" caption="SMTP prievadas" />
    <DataColumn name="TransportLogin" caption="SMTP prisijungimo vardas" />
    <DataColumn name="TransportPassword" caption="SMTP slaptažodis" />
  </columns>
</Grid>
