<?xml version="1.0" encoding="UTF-8"?>
<g:Grid 
  xmlns="http://www.butent.com/column" 
  xmlns:g="http://www.butent.com/grid"
  xmlns:ui="http://www.butent.com/ui" 
  
  name="Newsletters" viewName="Newsletters"
  caption="=mailNewsletters"
  >
  <g:columns>
    <IdColumn name="Id" />
    <DataColumn name="Subject" visible="true" />
    <CalcColumn name="FinishedDate" type="datetime" label="=date">
      <render>
        <ui:expression>(row.CountEmptyDate &gt; 0) ? null : row.MaxDate</ui:expression>
      </render>
    </CalcColumn>
    <DataColumn name="Active" editInPlace="true" />
  </g:columns>
</g:Grid>