<?xml version="1.0" encoding="UTF-8"?>
<Grid xmlns="http://www.butent.com/grid"

  name="EcDiscounts" viewName="EcDiscounts"
  flexGrow="1" flexShrink="1"
  >
  <columns>
    <DataColumn name="DateFrom" />
    <DataColumn name="DateTo" />

    <RelColumn name="Category" renderColumns="CategoryName" />
    <RelColumn name="Brand" renderColumns="BrandName" />

    <RelColumn name="ArticleName" label="=ecItemName" caching="none"
        renderColumns="ArticleName" searchableColumns="ArticleName"
        choiceColumns="ArticleName ArticleBrandName AnalogNr Supplier SupplierID" />
    <DataColumn name="ArticleBrandName" />
    <DataColumn name="AnalogNr" />
    <DataColumn name="Supplier" />
    <DataColumn name="SupplierID" />

    <DataColumn name="Percent" visible="true" />
    <DataColumn name="Price" visible="true" />

    <DataColumn name="Cost" visible="false" />
    <DataColumn name="Note" visible="false" />
  </columns>
</Grid>
