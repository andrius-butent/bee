<?xml version="1.0" encoding="UTF-8"?>
<Grid xmlns="http://www.butent.com/grid"

  name="EcOrderItems" viewName="EcOrderItems"
  flexGrow="1" flexShrink="1"
  >
  <columns>
    <RelColumn name="ArticleName" label="=ecItemName" caching="none"
        renderColumns="ArticleName" searchableColumns="ArticleName"
        choiceColumns="ArticleName BrandName AnalogNr Supplier SupplierID" />
    <DataColumn name="BrandName" />
    <DataColumn name="AnalogNr" />
    <DataColumn name="Supplier" />
    <DataColumn name="SupplierID" />
      
    <DataColumn name="QuantityOrdered" visible="true" />
    <DataColumn name="QuantitySubmit" visible="true" />

    <DataColumn name="Price" visible="true" />
    <DataColumn name="Cost" />

    <DataColumn name="Note" />
  </columns>
</Grid>
