<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"
  
  name="TcdTypes"
  idName="TypeID"
  audit="false"
  >
  <Fields>
    <Relation name="Model" relation="TcdModels" notNull="true" cascade="DELETE" label="=tcdModel" />
    <String name="TypeName" precision="50" notNull="true" label="=tcdType" />
    
    <Integer name="ProducedFrom" />
    <Integer name="ProducedTo" />
    <Integer name="Ccm" />
    <Numeric name="KwFrom" precision="3" />
    <Numeric name="KwTo" precision="3" />
    <Numeric name="Cylinders" precision="2" />
    <Numeric name="MaxWeight" precision="6" scale="2" />
    <String name="Engine" precision="50" />
    <String name="Fuel" precision="50" />
    <String name="Body" precision="50" />
    <String name="Axle" precision="50" />
    
    <Long name="TecDocID" unique="true" />
  </Fields>
</Table>
