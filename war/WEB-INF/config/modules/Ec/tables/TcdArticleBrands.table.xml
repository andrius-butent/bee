    <Relation name="Brand" relation="TcdBrands" notNull="true" label="=ecItemBrand" />
    <String name="AnalogNr" precision="50" notNull="true" label="=ecItemCode" />
    <Numeric name="Cost" precision="10" scale="2" label="=cost" />
    <Numeric name="UpdatedCost" precision="10" scale="2" label="=ecUpdatedCost" />
    <DateTime name="UpdateTime" label="=ecUpdateTime" />
    <String name="Supplier" precision="10" notNull="true" label="=ecItemSupplier"/>
    <String name="SupplierID" precision="30" notNull="true" label="=ecItemSupplierCode" />
  </Fields>
  <Constraints>
    <Unique fields="Supplier SupplierID" />
  </Constraints>
  <Indexes>
    <Index fields="Supplier" />
    <Index fields="SupplierID" />
  </Indexes>
</Table>
