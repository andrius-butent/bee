<?xml version="1.0" encoding="UTF-8"?>
<View xmlns="http://www.butent.com/view"

  name="Configuration"
  source="Configuration"
  >
  <Columns>
    <SimpleColumn name="Company" />
    <SimpleJoin name="Company" joinType="LEFT">
      <SimpleColumn alias="CompanyName" name="Name" />
    </SimpleJoin>

    <SimpleColumn name="AppointmentType" />
    <SimpleJoin name="AppointmentType" joinType="LEFT">
      <SimpleColumn alias="TypeName" name="Name" />
    </SimpleJoin>

    <SimpleColumn name="TimeZone" />
    <SimpleJoin name="TimeZone" joinType="LEFT">
      <SimpleColumn alias="TimeZoneName" name="Name" />
    </SimpleJoin>

    <SimpleColumn name="Theme" />
    <SimpleJoin name="Theme" joinType="LEFT">
      <SimpleColumn alias="ThemeName" name="Name" />
    </SimpleJoin>
  </Columns>
</View>
