<?xml version="1.0" encoding="UTF-8"?>
<BeeTable xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/table.xsd"

  name="Attendees"
  idName="AttendeeID"
  >
  <BeeFields>
    <BeeField name="AttendeeType" type="LONG" notNull="true" relation="AttendeeTypes" />

    <BeeField name="Name" type="STRING" precision="100" notNull="true" unique="true" />
    <BeeField name="Description" type="TEXT" />

    <BeeField name="Person" type="LONG" relation="Persons" />
    <BeeField name="CompanyPerson" type="LONG" relation="CompanyPersons" />

    <BeeField name="Transparency" type="INTEGER" />
  </BeeFields>
</BeeTable>
