<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="Attendees"
  idName="AttendeeID"
  >
  <Fields>
    <Relation name="AttendeeType" notNull="true" relation="AttendeeTypes" />

    <String name="Name" precision="100" notNull="true" unique="true" />
    <String name="Location" precision="100" />
    <Text name="Description" />

    <Relation name="CompanyPerson" relation="CompanyPersons" />

    <Integer name="Transparency" />
    <Integer name="Ordinal" />
    
    <Relation name="Color" relation="Colors" label="Spalva" />
    <Relation name="Style" relation="AppointmentStyles" label="Stilius" />
  </Fields>
</Table>
