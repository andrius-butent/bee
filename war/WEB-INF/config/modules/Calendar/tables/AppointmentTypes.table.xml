<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="AppointmentTypes"
  idName="AppointmentTypeID"
  >
  <Fields>
    <String name="Name" precision="100" notNull="true" unique="true" label="=calAppointmentTypeName"/>
    <Text name="Description" label="=description"/>

    <Relation name="Theme" relation="Themes" label="=calTheme" />
    <Relation name="Style" relation="AppointmentStyles" label="=calAppointmentStyle" />

    <String name="AppointmentCreator" precision="30" label="=appointmentCreator" />
    <String name="AppointmentEditor" precision="30" label="=appointmentEditor" />

    <Text name="SimpleHeader" label="=appointmentRenderSimpleHeader" />
    <Text name="SimpleBody" label="=appointmentRenderSimpleBody" />

    <Text name="MultiHeader" label="=appointmentRenderMultiHeader" />
    <Text name="MultiBody" label="=appointmentRenderMultiBody" />

    <Text name="AppointmentCompact" label="=calCompactForm" />
    <Text name="AppointmentTitle" label="=calAppointmentTitle" />
  </Fields>
</Table>
