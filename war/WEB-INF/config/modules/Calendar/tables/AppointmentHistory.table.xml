<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="AppointmentHistory"
  idName="AppointmentHistoryID"
  >
  <Fields>
    <Relation name="Appointment" notNull="true" relation="Appointments" cascade="DELETE" label="=calAppointment" />
    <Relation name="Publisher" notNull="true" relation="Users" defExpr="CURRENT_USER" label="=user" />
    <DateTime name="PublishTime" notNull="true" defExpr="CURRENT_TIME" label="=calPublishTime" />

    <Enum name="Status" key="AppointmentStatus" label="=calAppointmentStatus" />
    <Integer name="Hours" label="=unitHours" />
    <Integer name="Minutes" label="=unitMinutes" />

    <Text name="Comment" label="=calComment" />
  </Fields>
</Table>
