<?xml version="1.0" encoding="UTF-8"?>
<BeeTable xmlns="http://www.butent.com/table"

  name="AppointmentHistory"
  idName="AppointmentHistoryID"
  >
  <BeeFields>
    <BeeField name="Appointment" type="LONG" notNull="true" relation="Appointments" cascade="DELETE" />
    <BeeField name="Publisher" type="LONG" notNull="true" relation="Users" defExpr="CURRENT_USER" />
    <BeeField name="PublishTime" type="DATETIME" notNull="true" defExpr="CURRENT_TIME" />

    <BeeField name="Status" type="INTEGER" />
    <BeeField name="Hours" type="INTEGER" />
    <BeeField name="Minutes" type="INTEGER" />

    <BeeField name="Comment" type="TEXT" />
  </BeeFields>
</BeeTable>
