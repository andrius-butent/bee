<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="AppointmentHistory"
  idName="AppointmentHistoryID"
  >
  <Fields>
    <Relation name="Appointment" notNull="true" relation="Appointments" cascade="DELETE" />
    <Relation name="Publisher" notNull="true" relation="Users" defExpr="CURRENT_USER" />
    <DateTime name="PublishTime" notNull="true" defExpr="CURRENT_TIME" />

    <Integer name="Status" />
    <Integer name="Hours" />
    <Integer name="Minutes" />

    <Text name="Comment" />
  </Fields>
</Table>
