<?xml version="1.0" encoding="UTF-8"?>
<Columns xmlns="http://www.butent.com/view">
  <SimpleJoin name="TradeAct" joinType="LEFT">
    <SimpleColumn name="Date" />
    <SimpleColumn name="Parent" alias="TAParent"/>
    <SimpleColumn name="Kind" />

    <SimpleJoin joinType="LEFT" name="Series">
      <SimpleColumn name="SeriesName" alias="TradeSeriesName" />
    </SimpleJoin>
    <SimpleColumn name="Number" alias="TradeNumber" />

    <SimpleJoin joinType="LEFT" name="Object">
      <SimpleColumn name="ObjectName" />
      <SimpleColumn name="ObjectAddress" />
    </SimpleJoin>

    <SimpleJoin name="Contract" joinType="LEFT">
      <SimpleColumn name="Name" alias="ContractName" />
      <SimpleColumn name="Number" alias="ContractNumber" />
    </SimpleJoin>

    <SimpleColumn name="RentProject" />
  </SimpleJoin>
</Columns>
