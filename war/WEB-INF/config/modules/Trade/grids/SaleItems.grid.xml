<?xml version="1.0" encoding="UTF-8"?>
<Grid xmlns="http://www.butent.com/grid"
  xmlns:ui="http://www.butent.com/ui"

  name="SaleItems" viewName="SaleItems"
  flexGrow="1" flexShrink="1"
  newRowCaption="Nauja prekÄ—"
  >
  <columns>
    <RelColumn name="Item" />
    <DataColumn name="Quantity" format="#.000" />
    <DataColumn name="UnitName" />
    <DataColumn name="Price" format="#.00" />
    <CalcColumn name="Amount" type="decimal" caption="Suma" format="#.00">
      <render>
        <ui:expression>row.Quantity * row.Price</ui:expression>
      </render>
    </CalcColumn>
    <DataColumn name="Note" />
  </columns>
</Grid>
