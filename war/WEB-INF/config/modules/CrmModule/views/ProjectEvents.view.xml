<?xml version="1.0" encoding="UTF-8"?>
<BeeView xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/view.xsd"

  name="ProjectEvents"
  source="ProjectEvents"
  >
  <BeeColumns>
    <BeeSimpleColumn expression="Project" />
    <BeeSimpleJoin expression="Publisher" joinType="LEFT">
      <BeeSimpleJoin expression="Person" joinType="LEFT">
        <BeeSimpleColumn name="PublisherFirstName" expression="FirstName" />
        <BeeSimpleColumn name="PublisherLastName" expression="LastName" />
      </BeeSimpleJoin>
    </BeeSimpleJoin>
    <BeeSimpleColumn expression="PublishTime" />
    <BeeSimpleColumn expression="Comment" />
    <BeeSimpleColumn expression="Event" />
    <BeeSimpleColumn expression="EventNote" />
  </BeeColumns>
</BeeView>
 