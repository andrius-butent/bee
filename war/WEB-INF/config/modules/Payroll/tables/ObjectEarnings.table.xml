<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Table xmlns="http://www.butent.com/table"
  
  name="ObjectEarnings"
  idName="ObjectEarningsID"
  >
  <Fields>
    <Relation name="Object" notNull="true" relation="Locations" cascade="DELETE"
      label="=object" />

    <Numeric name="Year" notNull="true" precision="4" label="=year" />
    <Numeric name="Month" notNull="true" precision="2" label="=month" />

    <DateTime name="Approved" label="=earningsApprovedDate" />
    <Relation name="ApprovedBy" relation="Users" label="=earningsApprovedBy" />
    <Numeric name="ApprovedAmount" precision="12" scale="2" label="=earningsApprovedAmount" />
    <Relation name="ApprovedCurrency" relation="Currencies" label="=earningsApprovedCurrency" />

    <DateTime name="Exported" label="=earningsExportedDate" />
    <Relation name="ExportedBy" relation="Users" label="=earningsExportedBy" />
    <Numeric name="ExportedAmount" precision="12" scale="2" label="=earningsExportedAmount" />
    <Relation name="ExportedCurrency" relation="Currencies" label="=earningsExportedCurrency" />

    <String name="Note" precision="100" label="=note" />
  </Fields>
</Table>
