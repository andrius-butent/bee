<?xml version="1.0" encoding="UTF-8"?>
<View xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/view.xsd"

  name="UserRoles"
  source="UserRoles"
  >
  <Columns>
    <SimpleColumn name="User" />
    <SimpleColumn name="Role" />
    <SimpleJoin name="Role" joinType="INNER">
      <SimpleColumn alias="RoleName" name="Name" />
    </SimpleJoin>
  </Columns>
  <Order>
    <OrderBy column="RoleName" />
  </Order>
</View>
 