<?xml version="1.0" encoding="UTF-8"?>
<BeeView xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/view.xsd"

  name="UserRoles"
  source="UserRoles"
  >
  <BeeColumns>
    <BeeSimpleColumn expression="User" />
    <BeeSimpleColumn expression="Role" />
    <BeeSimpleJoin expression="Role" joinType="INNER">
      <BeeSimpleColumn name="RoleName" expression="Name" />
    </BeeSimpleJoin>
  </BeeColumns>
  <BeeOrder>
    <OrderBy column="RoleName" />
  </BeeOrder>
</BeeView>
 