<?xml version="1.0" encoding="UTF-8"?>
<BeeView xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/view.xsd"

  name="CurrencyRates"
  source="CurrencyRates"
  >
  <BeeColumns>
    <BeeSimpleColumn expression="Currency" />
    <BeeSimpleColumn expression="Date" />
    <BeeSimpleColumn expression="Quantity" />
    <BeeSimpleColumn expression="Rate" />
    <BeeSimpleColumn expression="BaseCurrency" />
    <BeeSimpleJoin joinType="LEFT" expression="BaseCurrency">
      <BeeSimpleColumn expression="Name" name="BaseCurrencyName"/>
    </BeeSimpleJoin>
  </BeeColumns>
  <BeeOrder>
    <OrderBy column="Date" />
  </BeeOrder>
</BeeView>
 