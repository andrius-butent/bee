<?xml version="1.0" encoding="UTF-8"?>
<View xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/view.xsd"

  name="CurrencyRates"
  source="CurrencyRates"
  >
  <Columns>
    <SimpleColumn name="Currency" />
    <SimpleColumn name="Date" />
    <SimpleColumn name="Quantity" />
    <SimpleColumn name="Rate" />
    <SimpleColumn name="BaseCurrency" />
    <SimpleJoin joinType="LEFT" name="BaseCurrency">
      <SimpleColumn name="Name" alias="BaseCurrencyName"/>
    </SimpleJoin>
  </Columns>
  <Order>
    <OrderBy column="Date" />
  </Order>
</View>
 