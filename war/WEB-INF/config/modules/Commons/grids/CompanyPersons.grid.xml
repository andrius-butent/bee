<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Grid xmlns="http://www.butent.com/grid"
  
  name="CompanyPersons"
  viewName="CompanyPersons"
  minColumnWidth="100"
  >
  <columns>
    <RelColumn name="Person" minWidth="200" editEnabled="true" />
    <DataColumn name="Position" />
    <DataColumn name="Phone" />

    <RelColumn name="EmailId" choiceColumns="Email Label">
      <renderToken source="Label" suffix=" " />
      <renderToken source="Email" prefix="&lt;" suffix="&gt;" />
    </RelColumn>
  </columns>
</Grid>
