    <Relation name="Unit" notNull="true" relation="Units" label="Mato vienetas" />
    <String name="Article" precision="30" unique="true" label="Artikulas" />
    <String name="Barcode" precision="30" unique="true" label="Barkodas" />
    <Numeric name="Price" precision="15" scale="5" label="Pard.kaina" />
    <Relation name="Currency" relation="Currencies" label="Valiuta" />
    <Boolean name="Vat" label="PVM" />
    <Numeric name="VatPercent" precision="3" label="PVM %" />
