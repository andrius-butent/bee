<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="Items"
  idName="ItemID"
  >
  <Fields>
    <String name="Name" precision="100" notNull="true" />
    <Relation name="Unit" notNull="true" relation="Units" />
    <String name="Article" precision="30" unique="true" />
    <String name="Barcode" precision="30" unique="true" />
    <Numeric name="Price" precision="15" scale="5" />
    <Relation name="Currency" relation="Currencies" />
    <Boolean name="IsService" />
  </Fields>
  <States>
    <State>Marked</State>
  </States>
  <Keys>
    <Key>
      <KeyField>IsService</KeyField>
    </Key>
  </Keys>
</Table>
