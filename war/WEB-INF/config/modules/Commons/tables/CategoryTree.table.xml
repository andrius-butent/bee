<?xml version="1.0" encoding="UTF-8"?>
<BeeTable xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/table.xsd"

  name="CategoryTree"
  idName="CategoryID"
  >
  <BeeFields>
    <BeeField name="Name" type="STRING" precision="60" notNull="true" />
    <BeeField name="Parent" type="LONG" relation="CategoryTree" />
  </BeeFields>
  <BeeStates>
    <BeeState>Marked</BeeState>
  </BeeStates>
  <BeeKeys>
    <BeeKey unique="true">
      <KeyField>Parent</KeyField>
      <KeyField>Name</KeyField>
    </BeeKey>
  </BeeKeys>
</BeeTable>
