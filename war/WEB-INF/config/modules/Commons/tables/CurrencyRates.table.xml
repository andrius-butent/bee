<?xml version="1.0" encoding="UTF-8"?>
<BeeTable xmlns="http://www.butent.com/table"

  name="CurrencyRates"
  idName="RateID"
  >
  <BeeFields>
    <BeeField name="Currency" type="LONG" relation="Currencies" notNull="true" cascade="DELETE" />
    <BeeField name="Date" type="DATETIME" notNull="true" />
    <BeeField name="Quantity" type="INTEGER" notNull="true" />
    <BeeField name="Rate" type="DECIMAL" precision="10" scale="4" notNull="true" />
  </BeeFields>
  <BeeKeys>
    <BeeKey unique="true">
      <KeyField>Currency</KeyField>
      <KeyField>Date</KeyField>
    </BeeKey>
  </BeeKeys>
</BeeTable>
