<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="CurrencyRates"
  idName="RateID"
  >
  <Fields>
    <Relation name="Currency" relation="Currencies" notNull="true" cascade="DELETE" />
    <DateTime name="Date" notNull="true" label="Data" />
    <Integer name="Quantity" notNull="true" label="Kiekis" />
    <Numeric name="Rate" precision="10" scale="4" notNull="true" label="Kursas" />
  </Fields>
  <Keys>
    <Key unique="true">
      <KeyField>Currency</KeyField>
      <KeyField>Date</KeyField>
    </Key>
  </Keys>
</Table>
