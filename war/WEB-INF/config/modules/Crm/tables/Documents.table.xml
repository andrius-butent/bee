<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="Documents"
  idName="DocumentID"
  >
  <Fields>
    <DateTime name="DocumentDate" />
    <DateTime name="Expires" />
    <String name="Number" precision="30" />
    <String name="RegistrationNumber" precision="30" />
    <Relation name="Type" relation="DocumentTypes" />
    <Relation name="Group" relation="DocumentGroups" />
    <Relation name="Category" notNull="true" relation="DocumentTree" />
    <Relation name="Place" relation="DocumentPlaces" />
    <Relation name="Company" relation="Companies" />
    <Relation name="CompanyPerson" relation="CompanyPersons" />
    <Relation name="Project" relation="Projects" />
    <String name="Name" precision="100" notNull="true" />
    <Text name="Description" />
  </Fields>
</Table>
