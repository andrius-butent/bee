<?xml version="1.0" encoding="UTF-8"?>
<BeeTable xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/table.xsd"

  name="Files"
  idName="FileID"
  >
  <BeeFields>
    <BeeField name="FileDate" type="DATETIME" notNull="true" />
    <BeeField name="FileVersion" type="STRING" precision="30" />
    <BeeField name="Document" type="LONG" notNull="true" relation="Documents" />
    <BeeField name="Name" type="STRING" precision="100" notNull="true" />
    <BeeField name="Description" type="TEXT" />
    <BeeField name="Hash" type="STRING" precision="100" />
    <BeeField name="Mime" type="STRING" precision="100" />
  </BeeFields>
</BeeTable>
