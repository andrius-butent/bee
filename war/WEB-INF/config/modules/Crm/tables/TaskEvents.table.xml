<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="TaskEvents"
  idName="TaskEventID"
  >
  <Fields>
    <Relation name="Task" notNull="true" relation="Tasks" cascade="DELETE" label="=task" />
    <Relation name="Publisher" notNull="true" relation="Users" label="Naudotojas" />
    <DateTime name="PublishTime" notNull="true" />
    <Text name="Comment" label="Komentaras" />
    <Integer name="Event" notNull="true" />
    <Text name="EventNote" />
    <Relation name="EventDuration" unique="true" relation="EventDurations" editable="true" />
    <DateTime name="FinishTime" label="Pabaigos data" />
  </Fields>
</Table>
