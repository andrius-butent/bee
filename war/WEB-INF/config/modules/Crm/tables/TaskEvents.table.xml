<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="TaskEvents"
  idName="TaskEventID"
  >
  <Fields>
    <Relation name="Task" notNull="true" relation="Tasks" cascade="DELETE" />
    <Relation name="Publisher" notNull="true" relation="Users" />
    <DateTime name="PublishTime" notNull="true" />
    <Text name="Comment" />
    <Integer name="Event" notNull="true" />
    <String name="EventNote" precision="100" />
    <Relation name="EventDuration" unique="true" relation="EventDurations" editable="true" />
  </Fields>
  <Keys>
    <Key>
      <KeyField>Event</KeyField>
    </Key>
  </Keys>
</Table>
