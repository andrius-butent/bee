<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="Requests"
  idName="RequestID"
  >
  <Fields>
    <DateTime name="Date" notNull="true" defExpr="CURRENT_TIME" label="Data" />
    <Relation name="Customer" relation="Companies" notNull="true" label="Klientas" />
    <Relation name="CustomerPerson" relation="CompanyPersons" label="Kontaktinis asmuo" />
    <String name="Contacts" precision="50" label="Kontaktai" />
    <Relation name="Payer" relation="Companies" label="Mokėtojas" />
    <Relation name="Manager" relation="Users" label="Vadybininkas" />
    <Text name="Request" notNull="true" label="Užklausimas" />
    <Text name="Result" label="Rezultatas" />
    <Relation name="Creator" relation="Users" defExpr="CURRENT_USER" label="Autorius" />
    <DateTime name="Created" notNull="true" defExpr="CURRENT_TIME" label="Sukurtas" />
    <DateTime name="Finished" label="Užbaigtas" />
  </Fields>
</Table>