<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="DocumentFiles"
  idName="FileID"
  >
  <Fields>
    <Relation name="Document" relation="Documents" notNull="true" cascade="DELETE" />
    <Relation name="File" relation="Files" notNull="true" />
    <String name="Caption" precision="100" />
    <Text name="Description" />
    <DateTime name="FileDate" notNull="true" />
    <String name="FileVersion" precision="30" />
    <Relation name="Owner" notNull="true" relation="Users" defExpr="CURRENT_USER" />
  </Fields>
</Table>
