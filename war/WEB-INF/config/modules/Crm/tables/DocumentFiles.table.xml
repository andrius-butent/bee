<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="DocumentFiles"
  idName="FileID"
  >
  <Fields>
    <DateTime name="FileDate" notNull="true" />
    <String name="FileVersion" precision="30" />
    <Relation name="Document" notNull="true" relation="Documents" />
    <String name="Name" precision="100" notNull="true" />
    <Text name="Description" />
    <String name="Hash" precision="100" />
    <String name="Mime" precision="100" />
    <Relation name="Owner" notNull="true" relation="Users" />
  </Fields>
</Table>
