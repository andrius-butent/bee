<?xml version="1.0" encoding="UTF-8"?>
<Grid xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/grid.xsd"
  
  name="Files" viewName="Files" caption="Bylos">
  
  <columns>
    <DataColumn name="FileDate" source="FileDate" caption="Data" />
    <DataColumn name="FileVersion" source="FileVersion" caption="Versija" />
    <DataColumn name="Name" source="Name" caption="Pavadinimas" />
    <DataColumn name="Description" source="Description" caption="ApraÅ¡ymas" />
    <DataColumn name="Mime" source="Mime" caption="Tipas" />
    <DataColumn name="Hash" source="Hash" caption="Kodas" readOnly="true" />
    <CalcColumn name="Owner" type="string" caption="Registratorius"
      sortBy="OwnerLastName, OwnerFirstName" searchBy="OwnerFirstName, OwnerLastName">
      <calc>
        <function>return (row.OwnerFirstName || "") + " " + (row.OwnerLastName || "")</function>
      </calc>
    </CalcColumn>
  </columns>
</Grid>
