<?xml version="1.0" encoding="UTF-8"?>
<View xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/view.xsd"

  name="ProjectUsers"
  source="ProjectUsers"
  >
  <Columns>
    <SimpleColumn name="Project" />
    <SimpleColumn name="User" />
    <SimpleJoin name="User" joinType="LEFT">
      <SimpleJoin name="Person" joinType="LEFT">
        <SimpleColumn name="FirstName" />
        <SimpleColumn name="LastName" />
      </SimpleJoin>
    </SimpleJoin>
    <SimpleColumn name="LastAccess" />
  </Columns>
  <Order>
    <OrderBy column="LastName" />
    <OrderBy column="FirstName" />
  </Order>
</View>
 