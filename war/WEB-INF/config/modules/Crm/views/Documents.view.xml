<?xml version="1.0" encoding="UTF-8"?>
<BeeView xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/view.xsd"

  name="Documents"
  source="Documents"
  >
  <BeeColumns>
    <BeeSimpleColumn expression="DocumentDate" />
    <BeeSimpleColumn expression="Expires" />
    <BeeSimpleColumn expression="Number" />
    <BeeSimpleColumn expression="RegistrationNumber" />
    <BeeSimpleColumn expression="Type" />
    <BeeSimpleJoin joinType="LEFT" expression="Type">
      <BeeSimpleColumn name="TypeName" expression="Name" />
    </BeeSimpleJoin>
    <BeeSimpleColumn expression="Group" />
    <BeeSimpleJoin joinType="LEFT" expression="Group">
      <BeeSimpleColumn name="GroupName" expression="Name" />
    </BeeSimpleJoin>
    <BeeSimpleColumn expression="Category" />
    <BeeSimpleJoin joinType="LEFT" expression="Category">
      <BeeSimpleColumn name="CategoryName" expression="Name" />
    </BeeSimpleJoin>
    <BeeSimpleColumn expression="Place" />
    <BeeSimpleJoin joinType="LEFT" expression="Place">
      <BeeSimpleColumn name="PlaceName" expression="Name" />
    </BeeSimpleJoin>
    <BeeSimpleColumn expression="Project" />
    <BeeSimpleJoin expression="Project" joinType="LEFT">
      <BeeSimpleColumn expression="Name" name="ProjectName" />
    </BeeSimpleJoin>
    <BeeSimpleColumn expression="Company" />
    <BeeSimpleJoin expression="Company" joinType="LEFT">
      <BeeSimpleColumn name="CompanyName" expression="Name" />
    </BeeSimpleJoin>
    <BeeSimpleColumn expression="CompanyPerson" />
    <BeeSimpleJoin expression="CompanyPerson" joinType="LEFT">
      <BeeSimpleJoin expression="Person" joinType="LEFT">
        <BeeSimpleColumn name="PersonFirstName" expression="FirstName" />
        <BeeSimpleColumn name="PersonLastName" expression="LastName" />
      </BeeSimpleJoin>
    </BeeSimpleJoin>
    <BeeSimpleColumn expression="Name" />
    <BeeSimpleColumn expression="Description" />
  </BeeColumns>
</BeeView>
 