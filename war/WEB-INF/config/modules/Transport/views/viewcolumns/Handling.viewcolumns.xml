<?xml version="1.0" encoding="UTF-8"?>
<Columns xmlns="http://www.butent.com/view">
  <SimpleJoin name="LoadingPlace" joinType="LEFT">
    <SimpleColumn name="Date" alias="LoadingDate" label="=trLoadingDate" />
    <SimpleColumn name="Note" alias="LoadingNote" label="=trLoadingNote" />
    <SimpleColumn name="Number" alias="LoadingNumber" label="=trLoadingNumber" />
    <SimpleColumn name="Contact" alias="LoadingContact" label="=trLoadingContact" />
    <SimpleColumn name="Company" alias="LoadingCompany" label="=trLoadingCompany" />
    <SimpleColumn name="Address" alias="LoadingAddress" label="=trLoadingAddress" />
    <SimpleColumn name="PostIndex" alias="LoadingPostIndex" label="=trLoadingPostIndex" />

    <SimpleColumn name="City" alias="LoadingCity" label="=trLoadingCity" />
    <SimpleJoin joinType="LEFT" name="City">
      <SimpleColumn name="Name" alias="LoadingCityName" label="=trLoadingCity" />
      <SimpleColumn name="Name2" alias="LoadingCityNameEN" />
      <SimpleColumn name="Name3" alias="LoadingCityNameRU" />
    </SimpleJoin>
    <SimpleColumn name="Country" alias="LoadingCountry" label="=trLoadingCountry" />
    <SimpleJoin joinType="LEFT" name="Country">
      <SimpleColumn name="Name" alias="LoadingCountryName" label="=trLoadingCountry" />
      <SimpleColumn name="Name2" alias="LoadingCountryNameEN" />
      <SimpleColumn name="Name3" alias="LoadingCountryNameRU" />
      <SimpleColumn name="Code" alias="LoadingCountryCode" label="=from" />
    </SimpleJoin>
  </SimpleJoin>

  <SimpleJoin name="UnloadingPlace" joinType="LEFT">
    <SimpleColumn name="Date" alias="UnloadingDate" label="=trUnloadingDate" />
    <SimpleColumn name="Note" alias="UnloadingNote" label="=trUnloadingNote" />
    <SimpleColumn name="Number" alias="UnloadingNumber" label="=trUnloadingNumber" />
    <SimpleColumn name="Contact" alias="UnloadingContact" label="=trUnloadingContact" />
    <SimpleColumn name="Company" alias="UnloadingCompany" label="=trUnloadingCompany" />
    <SimpleColumn name="Address" alias="UnloadingAddress" label="=trUnloadingAddress" />
    <SimpleColumn name="PostIndex" alias="UnloadingPostIndex" label="=trUnloadingPostIndex" />

    <SimpleColumn name="City" alias="UnloadingCity" label="=trUnloadingCity" />
    <SimpleJoin joinType="LEFT" name="City">
      <SimpleColumn name="Name" alias="UnloadingCityName" label="=trUnloadingCity" />
      <SimpleColumn name="Name2" alias="UnloadingCityNameEN" />
      <SimpleColumn name="Name3" alias="UnloadingCityNameRU" />
    </SimpleJoin>
    <SimpleColumn name="Country" alias="UnloadingCountry" label="=trUnloadingCountry" />
    <SimpleJoin joinType="LEFT" name="Country">
      <SimpleColumn name="Name" alias="UnloadingCountryName" label="=trUnloadingCountry" />
      <SimpleColumn name="Name2" alias="UnloadingCountryNameEN" />
      <SimpleColumn name="Name3" alias="UnloadingCountryNameRU" />
      <SimpleColumn name="Code" alias="UnloadingCountryCode" label="=to" />
    </SimpleJoin>
  </SimpleJoin>
</Columns>
