<?xml version="1.0" encoding="UTF-8"?>
<BeeView xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/view.xsd"

  name="OrderServices"
  source="OrderServices"
  >
  <BeeColumns>
    <BeeSimpleColumn expression="Order"/>
    <BeeSimpleColumn expression="Item"/>
    <BeeSimpleJoin joinType="LEFT" expression="Item">
      <BeeSimpleColumn expression="Name" name="ItemName" />
      <BeeSimpleJoin joinType="LEFT" expression="Unit">
        <BeeSimpleColumn expression="Name" name="UnitName" />
      </BeeSimpleJoin>
    </BeeSimpleJoin>
    <BeeSimpleColumn expression="Quantity" />
    <BeeSimpleColumn expression="Price" />
    <BeeSimpleColumn expression="Currency" />
    <BeeSimpleJoin joinType="LEFT" expression="Currency">
      <BeeSimpleColumn expression="Name" name="CurrencyName" />
    </BeeSimpleJoin>
  </BeeColumns>
</BeeView>
