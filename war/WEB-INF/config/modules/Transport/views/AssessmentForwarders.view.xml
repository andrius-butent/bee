<?xml version="1.0" encoding="UTF-8"?>
<View xmlns="http://www.butent.com/view"

      name="AssessmentForwarders" source="AssessmentForwarders" caption="=carriers"
    >
  <Columns>
    <SimpleColumn name="Assessment" />

    <SimpleColumn name="CargoTrip" />
    <SimpleJoin name="CargoTrip" joinType="INNER">
      <SimpleColumn name="CargoHandling" />
      <SimpleJoin name="CargoHandling" joinType="LEFT">
        <SimpleJoin name="LoadingPlace" joinType="LEFT">
          <SimpleColumn name="Date" alias="LoadingDate" label="=trLoadingDate" />
          <SimpleColumn name="Note" alias="LoadingNote" label="=trLoadingNote" />
          <SimpleColumn name="Number" alias="LoadingNumber" label="=trLoadingNumber" />
          <SimpleColumn name="Contact" alias="LoadingContact" label="=trLoadingContact" />
          <SimpleColumn name="Company" alias="LoadingCompany" label="=trLoadingCompany" />
          <SimpleColumn name="Address" alias="LoadingAddress" label="=trLoadingAddress" />
          <SimpleColumn name="PostIndex" alias="LoadingPostIndex" label="=trLoadingPostIndex" />

          <SimpleColumn name="City" alias="LoadingCity" label="=trLoadingCity" />
          <SimpleJoin joinType="LEFT" name="City">
            <SimpleColumn name="Name" alias="LoadingCityName" />
          </SimpleJoin>
          <SimpleColumn name="Country" alias="LoadingCountry" label="=trLoadingCountry" />
          <SimpleJoin joinType="LEFT" name="Country">
            <SimpleColumn name="Name" alias="LoadingCountryName" />
            <SimpleColumn name="Code" alias="LoadingCountryCode" />
          </SimpleJoin>
        </SimpleJoin>

        <SimpleJoin name="UnloadingPlace" joinType="LEFT">
          <SimpleColumn name="Date" alias="UnloadingDate" label="=trUnloadingDate" />
          <SimpleColumn name="Note" alias="UnloadingNote" label="=trUnloadingNote" />
          <SimpleColumn name="Number" alias="UnloadingNumber" label="=trUnloadingNumber" />
          <SimpleColumn name="Contact" alias="UnloadingContact" label="=trUnloadingContact" />
          <SimpleColumn name="Company" alias="UnloadingCompany" label="=trUnloadingCompany" />
          <SimpleColumn name="Address" alias="UnloadingAddress" label="=trUnloadingAddress" />
          <SimpleColumn name="PostIndex" alias="UnloadingPostIndex" label="=trUnloadingPostIndex" />

          <SimpleColumn name="City" alias="UnloadingCity" label="=trUnloadingCity" />
          <SimpleJoin joinType="LEFT" name="City">
            <SimpleColumn name="Name" alias="UnloadingCityName" />
          </SimpleJoin>
          <SimpleColumn name="Country" alias="UnloadingCountry" label="=trUnloadingCountry" />
          <SimpleJoin joinType="LEFT" name="Country">
            <SimpleColumn name="Name" alias="UnloadingCountryName" />
            <SimpleColumn name="Code" alias="UnloadingCountryCode" />
          </SimpleJoin>
        </SimpleJoin>
      </SimpleJoin>

      <SimpleJoin joinType="LEFT" name="Trip">
        <SimpleColumn name="Expedition" editable="true" />
        <SimpleJoin joinType="LEFT" name="Expedition">
          <SimpleColumn name="Name" alias="ExpeditionType" label="=trExpeditionType" />
        </SimpleJoin>

        <SimpleColumn name="Forwarder" editable="true" />
        <SimpleJoin joinType="LEFT" name="Forwarder">
          <SimpleColumn name="Name" alias="ForwarderName" label="=carrier" />
          <SimpleJoin joinType="LEFT" name="CompanyType">
            <SimpleColumn name="Name" alias="ForwarderTypeName" />
          </SimpleJoin>
        </SimpleJoin>

        <SimpleColumn name="ForwarderVehicle" editable="true" />
        <SimpleColumn name="Notes" label="=transportOtherDetails" editable="true" />
      </SimpleJoin>
    </SimpleJoin>

    <SimpleJoin joinType="INNER" name="CargoExpense">
      <SimpleColumn name="Cargo" alias="ServiceCargo" />

      <SimpleColumn name="Service" editable="true" />
      <SimpleJoin joinType="LEFT" name="Service">
        <SimpleColumn name="Name" alias="ServiceName" />
      </SimpleJoin>
      <SimpleColumn name="Amount" editable="true" />
      <SimpleColumn name="Currency" editable="true" />
      <SimpleJoin joinType="LEFT" name="Currency">
        <SimpleColumn name="Name" alias="CurrencyName" />
      </SimpleJoin>
      <SimpleColumn name="VatPlus" />
      <SimpleColumn name="Vat" />
      <SimpleColumn name="VatPercent" />
      <SimpleColumn name="Supplier" editable="true" />
      <SimpleJoin name="Supplier" joinType="LEFT">
        <SimpleColumn name="Name" alias="SupplierName" />
        <SimpleJoin joinType="LEFT" name="CompanyType">
          <SimpleColumn name="Name" alias="SupplierTypeName" />
        </SimpleJoin>
      </SimpleJoin>
      <SimpleColumn name="Date" editable="true" />
      <SimpleColumn name="Number" editable="true" />
      <SimpleColumn name="Note" label="=transportConditions" editable="true" />
      <SimpleColumn name="Income" editable="true" />
    </SimpleJoin>
  </Columns>
</View>
