<?xml version="1.0" encoding="UTF-8"?>
<View xmlns="http://www.butent.com/view"

  name="FuelConsumptions"
  source="FuelConsumptions"
  >
  <Columns>
    <SimpleColumn name="Vehicle"/>
    <SimpleColumn name="DateFrom"/>
    <SimpleColumn name="DateTo"/>
    <SimpleColumn name="Fuel"/>
    <SimpleJoin joinType="LEFT" name="Fuel">
      <SimpleColumn name="Name" alias="FuelType" />
    </SimpleJoin>
    <SimpleColumn name="Summer" />
    <SimpleColumn name="Winter" />
    <SimpleColumn name="TonneKilometer" />
    <SimpleColumn name="MotoHour" />
  </Columns>
  <Order>
    <OrderBy column="DateFrom" descending="true"/>
  </Order>
</View>
