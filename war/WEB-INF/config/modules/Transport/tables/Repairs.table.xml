<?xml version="1.0" encoding="UTF-8"?>
<BeeTable xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/table.xsd"

  name="Repairs"
  idName="RepairID"
  x="0" y="0"
  >
  <BeeFields>
    <BeeField name="Vehicle" type="LONG" notNull="true" relation="Vehicles" cascade="DELETE" />
    <BeeField name="Date" type="DATE" />
    <BeeField name="Item" type="LONG" notNull="true" relation="Items" />
    <BeeField name="Unit" type="LONG" notNull="true" relation="Units" />
    <BeeField name="Quantity" type="DOUBLE" notNull="true" />
    <BeeField name="Price" type="DECIMAL" precision="15" scale="5" />
    <BeeField name="Currency" type="LONG" relation="Currencies" />
    <BeeField name="Note" type="STRING" precision="50" />
    <BeeField name="Run" type="INTEGER" />
    <BeeField name="DocumentNo" type="STRING" precision="30" />
    <BeeField name="Supplier" type="LONG" relation="Companies" />
    <BeeField name="Country" type="LONG" relation="Countries" />
  </BeeFields>
</BeeTable>