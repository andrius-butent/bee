<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

       name="CargoPlaces"
       idName="PlaceID"
>
  <Fields>
    <Numeric name="Ordinal" precision="2" scale="0" label="=ordinal" />

    <DateTime name="Date" label="=date" />
    <String name="Number" precision="50" label="=ref" />
    <String name="Contact" precision="50" label="=contact" />
    <String name="Company" precision="100" label="=company" />
    <String name="Address" precision="100" label="=address" />
    <String name="PostIndex" precision="20" label="=postIndex" />
    <Relation name="City" relation="Cities" label="=city" />
    <Relation name="Country" relation="Countries" label="=country" />
    <Text name="Note" label="=note" />

    <Integer name="EmptyKilometers" label="=trEmptyKilometers" />
    <Integer name="LoadedKilometers" label="=trLoadedKilometers" />

    <Integer name="UnplannedManagerKm" label="Neplanuoti km (vadb)" />
    <Integer name="UnplannedDriverKm" label="Neplanuoti km (vair)" />

    <Numeric name="Weight" precision="10" scale="3" label="=trWeightInTons" />
  </Fields>
</Table>