<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="Vehicles"
  idName="VehicleID"
  x="0" y="0"
  >
  <Fields>
    <Relation name="Type" notNull="true" relation="VehicleTypes" />
    <Relation name="Model" notNull="true" relation="VehicleModels" label="=vehicleModel" />
    <String name="Number" precision="10" unique="true" notNull="true" />
    <String name="BodyNumber" precision="20" unique="true" />
    <String name="EngineNumber" precision="20" unique="true" />
    <Numeric name="EngineVolume" precision="5" />
    <Date name="ProductionDate" />
    <Date name="StartDate" label="=trVehicleStartDate" />
    <Date name="EndDate" label="=trVehicleEndDate" />
    <Relation name="Owner" relation="Companies" />
    <Integer name="Speedometer" />
    <Relation name="Fuel" relation="FuelTypes" />
    <Numeric name="TankCapacity" precision="4" />
    <Numeric name="Power" precision="4" />
    <Double name="Netto" />
    <Double name="Brutto" />
    <Text name="Notes" label="=notes" />
  </Fields>
  <Extensions>
    <Numeric name="Body" precision="1" />
    <Numeric name="Chassis" precision="1" />
    <Numeric name="Gearbox" precision="1" />
    <Boolean name="Conditioner" />
  </Extensions>
</Table>