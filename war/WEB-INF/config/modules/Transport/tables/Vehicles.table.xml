<?xml version="1.0" encoding="UTF-8"?>
<BeeTable xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../../../../schemas/table.xsd"

  name="Vehicles"
  idName="VehicleID"
  x="0" y="0"
  >
  <BeeFields>
    <BeeField name="Type" type="LONG" notNull="true" relation="VehicleTypes" />
    <BeeField name="Model" type="LONG" notNull="true" relation="VehicleModels" />
    <BeeField name="Number" type="STRING" precision="10" unique="true" notNull="true" />
    <BeeField name="BodyNumber" type="STRING" precision="20" unique="true" />
    <BeeField name="EngineNumber" type="STRING" precision="20" unique="true" />
    <BeeField name="EngineVolume" type="DECIMAL" precision="5" />
    <BeeField name="ProductionDate" type="DATE" />
    <BeeField name="Owner" type="LONG" relation="Companies" />
    <BeeField name="Speedometer" type="INTEGER" />
    <BeeField name="TankCapacity" type="DECIMAL" precision="4" />
    <BeeField name="Power" type="DECIMAL" precision="4" />
    <BeeField name="Netto" type="DOUBLE" />
    <BeeField name="Brutto" type="DOUBLE" />
    <BeeField name="Notes" type="TEXT" />
  </BeeFields>
</BeeTable>