<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="OrderCargo"
  idName="CargoID"
  x="741" y="101"
  >
  <Fields>
    <Relation name="Order" relation="TransportationOrders" notNull="true" cascade="DELETE" />
    <String name="Description" precision="100" notNull="true" />
    <Numeric name="Price" precision="12" scale="2" />
    <Relation name="Currency" relation="Currencies" />

    <Numeric name="Weight" precision="7" scale="3" />
    <Numeric name="Volume" precision="7" scale="3" />
    <Numeric name="LDM" precision="5" scale="2" />
    <Numeric name="Length" precision="6" scale="2" />
    <Numeric name="Width" precision="6" scale="2" />
    <Numeric name="Height" precision="6" scale="2" />
    <Integer name="Quantity" />
    <Integer name="Value" />

    <String name="Number" precision="20" />
    <String name="Cmr" precision="20" />
    <Integer name="LoadedKilometers" />
    <Integer name="EmptyKilometers" />

    <Relation name="LoadingPlace" unique="true" relation="CargoPlaces" editable="true" />
    <Relation name="UnloadingPlace" unique="true" relation="CargoPlaces" editable="true" />
    <Text name="Notes" />
  </Fields>
</Table>