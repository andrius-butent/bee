<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="TripRoutes"
  idName="RouteID"
  x="0" y="0"
  >
  <Fields>
    <Relation name="Trip" notNull="true" relation="Trips" cascade="DELETE" />
    <Date name="Date" notNull="true" />
    <Relation name="Country" relation="Countries" />
    <String name="Route" precision="100" />
    <Double name="SpeedometerFrom" />
    <Double name="SpeedometerTo" />
    <Double name="Kilometers" />
    <Numeric name="MotoHours" precision="7" scale="2" />
    <Numeric name="CargoWeight" precision="7" scale="3" />
    <Numeric name="Season" precision="1" notNull="true" defValue="0" />
    <Numeric name="Temperature" precision="3" scale="0" />
    <Double name="Consumption" />
    <Relation name="Item" relation="Items" />
  </Fields>
</Table>