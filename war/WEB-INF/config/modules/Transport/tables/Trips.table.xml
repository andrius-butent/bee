<?xml version="1.0" encoding="UTF-8"?>
<BeeTable xmlns="http://www.butent.com/table"

  name="Trips"
  idName="TripID"
  x="0" y="0"
  >
  <BeeFields>
    <BeeField name="Date" type="DATETIME" notNull="true" defExpr="CURRENT_TIME" />
    <BeeField name="TripNo" type="STRING" precision="20" unique="true" notNull="true"
      defExpr="NEXT_NUMBER" defValue="R/" />
    <BeeField name="Vehicle" type="LONG" relation="Vehicles" />
    <BeeField name="Trailer" type="LONG" relation="Vehicles" />
    <BeeField name="DateFrom" type="DATE" />
    <BeeField name="DateTo" type="DATE" />
    <BeeField name="SpeedometerBefore" type="DOUBLE" />
    <BeeField name="SpeedometerAfter" type="DOUBLE" />
    <BeeField name="FuelBefore" type="DECIMAL" precision="7" scale="2" />
    <BeeField name="FuelAfter" type="DECIMAL" precision="7" scale="2" />
    <BeeField name="Forwarder" type="LONG" relation="Companies" />
    <BeeField name="ForwarderVehicle" type="STRING" precision="50" />
    <BeeField name="Notes" type="TEXT" />
  </BeeFields>
</BeeTable>