<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Table xmlns="http://www.butent.com/table"
  
  name="ShipmentRequests"
  idName="ShipmentRequestID"
  >
  <Fields>
    <DateTime name="Created" notNull="true" defExpr="CURRENT_TIME" label="=trERcreationDate" />
  
    <String name="CustomerName" precision="100" notNull="true" label="=trERcustomerName" />
    <String name="CustomerCode" precision="30" label="=trERcustomerCode" />
    <String name="CustomerVatCode" precision="30" label="=trERcustomerVatCode" />

    <String name="CustomerAddress" precision="100" label="=trERcustomerAddress" />
    <String name="CustomerPhone" precision="50" label="=trERcustomerPhone" />
    <String name="CustomerEmail" precision="255" label="=trERcustomerEmail" />

    <String name="CustomerContact" precision="50" label="=trERcustomerContact" />
    <String name="CustomerContactPosition" precision="50" label="=trERcustomerContactPosition" />

    <String name="CustomerExchangeCode" precision="30" label="=trERcustomerExchangeCode" />

    <String name="LoadingCompanyName" precision="100" label="=trLoadingCompanyName" />
    <String name="LoadingEmail" precision="255" label="=trLoadingEmail" />
    <String name="LoadingCity" precision="30" label="=trLoadingCity" />

    <String name="UnloadingCompanyName" precision="100" label="=trUnloadingCompanyName" />
    <String name="UnloadingEmail" precision="255" label="=trUnloadingEmail" />
    <String name="UnloadingCity" precision="30" label="=trUnloadingCity" />

    <Relation name="Expedition" relation="ExpeditionTypes" label="=trERexpeditionType" />

    <DateTime name="DeliveryTime" label="=trERdeliveryTime" />
    <String name="TermsOfDelivery" precision="30" label="=trERtermsOfDelivery" />
    
    <Boolean name="CustomsBrokerage" label="=trERcustomsBrokerage" />
    <Boolean name="FreightInsurance" label="=trERfreightInsurance" />

    <Relation name="Cargo" relation="OrderCargo" cascade="DELETE" label="=cargo" />

    <Relation name="Manager" relation="Users" label="=trERresponsibleManager" />

    <Text name="Notes" label="=trERnotes" />
    <Text name="Host" label="=trERhost" />
  </Fields>
</Table>
