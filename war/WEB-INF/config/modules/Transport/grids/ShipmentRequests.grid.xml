    <DataColumn name="CustomerExchangeCode" visible="false" />
    
    <DataColumn name="LoadingCompanyName" visible="false" />
    <DataColumn name="LoadingContact" visible="false" />
    <DataColumn name="LoadingDate" />

    <DataColumn name="LoadingCity" />
    <CalcColumn type="string" name="From" source="LoadingCountryCode" cellType="html" width="20"
      caption="=from" horizontalAlignment="center">
      <renderer type="flag" />
    </CalcColumn>

    <DataColumn name="LoadingAddress" visible="false" />
    <DataColumn name="LoadingEmail" visible="false" />
    <DataColumn name="LoadingPhone" visible="false" />
    <DataColumn name="LoadingFax" visible="false" />
    <DataColumn name="LoadingNumber" visible="false" />

    <DataColumn name="UnloadingCompanyName" visible="false" />
    <DataColumn name="UnloadingContact" visible="false" />
    <DataColumn name="UnloadingDate" />

    <DataColumn name="UnloadingCity" />
    <CalcColumn type="string" name="To" source="UnloadingCountryCode" cellType="html" width="20"
      caption="=to" horizontalAlignment="center">
      <renderer type="flag" />
    </CalcColumn>

    <DataColumn name="UnloadingAddress" visible="false" />
    <DataColumn name="UnloadingEmail" visible="false" />
    <DataColumn name="UnloadingPhone" visible="false" />
    <DataColumn name="UnloadingFax" visible="false" />
    <DataColumn name="UnloadingNumber" visible="false" />
    
    <RelColumn name="Expedition" visible="false" />
    <RelColumn name="ShippingTerm" visible="false" />

    <DataColumn name="DeliveryTime" visible="false" />
    <DataColumn name="TermsOfDelivery" visible="false" />

    <DataColumn name="CustomsBrokerage" visible="false" />
    <DataColumn name="FreightInsurance" visible="false" />

    <DataColumn name="Description" caption="=cargo" whiteSpace="nowrap" />
    <DataColumn name="Quantity" visible="false" />

    <DataColumn name="Weight" visible="false" />
    <DataColumn name="Volume" visible="false" />
    <DataColumn name="LDM" visible="false" />
    <DataColumn name="Length" visible="false" />
    <DataColumn name="Width" visible="false" />
    <DataColumn name="Height" visible="false" />
    <DataColumn name="Palettes" visible="false" />

    <DataColumn name="Value" />
    <RelColumn name="ValueCurrency" />
    
    <RelColumn name="Manager" />

    <DataColumn name="Notes" visible="false" />
    <DataColumn name="Host" visible="false" />
  </g:columns>
</g:Grid>
