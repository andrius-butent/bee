<?xml version="1.0" encoding="UTF-8"?>
<g:Grid xmlns="http://www.butent.com/column" xmlns:g="http://www.butent.com/grid"

  name="ShipmentRequests" viewName="ShipmentRequests"
  caption="=trERgridCaption" order="Created desc"
  >
  <g:columns>
    <DataColumn name="Created" />

    <DataColumn name="CustomerName" visible="true" />
    <DataColumn name="CustomerCode" visible="false" />
    <DataColumn name="CustomerVatCode" visible="false" />

    <DataColumn name="CustomerAddress" visible="false" />
    <DataColumn name="CustomerPhone" />
    <DataColumn name="CustomerEmail" />

    <DataColumn name="CustomerContact" />
