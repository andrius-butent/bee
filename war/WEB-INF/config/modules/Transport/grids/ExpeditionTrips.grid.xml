<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Grid xmlns="http://www.butent.com/grid"
  xmlns:ui="http://www.butent.com/ui"

  name="ExpeditionTrips" viewName="ExpeditionTrips" caption="Ekspedijavimo reisai"
  newRowPopup="true"  
  >

  <columns>
    <DataColumn name="Date" caption="Data" />
    <DataColumn name="ExpeditionType" caption="Ekspedijavimo rūšis" />
    <DataColumn name="TripNo" caption="Reiso Nr." />
    <DataColumn name="ForwarderName" caption="Vežėjas" flexGrow="1" flexShrink="1" />
    <DataColumn name="ForwarderVehicle" caption="Transportas" flexGrow="1" flexShrink="1" />
    <DataColumn name="ForwarderDriver" caption="Vairuotojas" flexGrow="1" flexShrink="1" />
    <DataColumn name="DateFrom" source="DateFrom" caption="Išvyko" width="80" />
    <DataColumn name="DateTo" source="DateTo" caption="Sugrįžo" width="80" />
    <DataColumn name="Notes" caption="Pastabos">
      <style>
        <bodyStyle>
          <ui:inline>white-space: pre-wrap;</ui:inline>
        </bodyStyle>
      </style>
    </DataColumn>
  </columns>
</Grid>
