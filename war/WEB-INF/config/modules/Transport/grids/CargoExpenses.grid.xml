<?xml version="1.0" encoding="UTF-8"?>
<Grid xmlns="http://www.butent.com/grid"
  xmlns:ui="http://www.butent.com/ui"

  name="CargoExpenses" viewName="CargoExpenses"
  flexGrow="1"
  newRowCaption="=trNewCargoExpense"
  >
  <columns>
    <DataColumn name="Date" />

    <RelColumn name="Service" />
    <DataColumn name="Amount" format="#.00">
      <style>
        <headerStyle><ui:font>bold</ui:font></headerStyle>
        <bodyStyle><ui:font>bold</ui:font></bodyStyle>
      </style>
    </DataColumn>
    <RelColumn name="Currency" flexGrow="0" />
    <DataColumn name="Vat" />
    <DataColumn name="VatPercent" width="20" sortable="false" />

    <RelColumn name="Supplier" />
    <DataColumn name="Number" />
    <DataColumn name="Note" />
  </columns>
</Grid>
