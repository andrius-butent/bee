<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Grid xmlns="http://www.butent.com/grid"
  xmlns:ui="http://www.butent.com/ui"

  name="CargoServices" viewName="CargoServices"
  >
  <header borderWidth="1 0" />
  <body height="20" borderWidth="0">
    <style>
      <ui:font>Verdana 12 px</ui:font>
    </style>
  </body>
  <footer borderWidth="0" />

  <columns>
    <RelColumn name="Item" caption="Paslauga" newRowCaption="Nauja paslauga" />
    <RelColumn name="Unit" caption="Mato v." />
    <DataColumn name="Quantity" caption="Kiekis" />
    <DataColumn name="Price" caption="Kaina" format="#.00" />
    <RelColumn name="Currency" caption="Val." />
    <CalcColumn type="decimal" name="Total" caption="Suma" format="#.00">
      <style>
        <headerStyle><ui:font>bold</ui:font></headerStyle>
        <bodyStyle><ui:font>bold</ui:font></bodyStyle>
      </style>
      <render><ui:expression>row.Quantity * row.Price</ui:expression></render>
    </CalcColumn>
    <DataColumn name="CostPrice" caption="Savikaina" format="#.00" />
  </columns>
</Grid>
