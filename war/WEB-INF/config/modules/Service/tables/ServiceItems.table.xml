<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

       name="ServiceItems"
       idName="ServiceItemsID"
>
  <Fields>
    <Relation name="ServiceObject" notNull="true" relation="ServiceObjects" cascade="DELETE"
              label="=svcObject" />

    <Relation name="ServiceMaintenance" relation="ServiceMaintenance" cascade="DELETE"
              label="=svcMaintenance" />

    <Relation name="Repairer" relation="CompanyPersons" label="=svcMaster"/>
    <Boolean name="Reserve" label="=reserve" defValue="1" />
    <String name="Article" precision="30" label="=article" />

    <Relation name="ExtraDimensions" unique="true" relation="ExtraDimensions" editable="true"
              label="=dimensions" />
  </Fields>
</Table>