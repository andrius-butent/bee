<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="ServiceObjects"
  idName="ObjectID"
  >
  <Fields>
    <Relation name="Category" relation="ServiceTree" notNull="true" label="=category" />
    <String name="Address" precision="100" notNull="true" label="=address" />
    <Enum name="Status" key="SvcObjectStatus" notNull="true" defValue="0"
      label="=svcObjectStatus" />

    <String name="Number" precision="256" label="=svcObjectNumber"/>
    <String name="Name" precision="256" label="=svcObjectName" />

    <Relation name="Customer" relation="Companies" label="=customer" />
    <Relation name="Contractor" relation="Companies" label="=svcContractor" />
    <Relation name="Project" relation="Projects" label="=project" />
  </Fields>
</Table>
