<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="ServiceObjects"
  idName="ObjectID"
  >
  <Fields>
    <Relation name="Category" relation="ServiceTree" notNull="true" label="=category" />
    <String name="Address" precision="100" notNull="true" label="Valst.Nr" />

    <Relation name="Customer" relation="Companies" label="=svcOwner" />
    <Relation name="Contractor" relation="Companies" label="=svcUser" />
    
    <Long name="ExternalCode" unique="true" label="=externalCode" />
    
    <String name="Model" precision="100" />
    <String name="YearOfManufacture" precision="100" />
    <String name="InventNo" precision="100" />
    <String name="BodyNo" precision="100" />
    <String name="Seller" precision="100" />
    <String name="ItemInButenta" precision="100" />
  </Fields>
</Table>
