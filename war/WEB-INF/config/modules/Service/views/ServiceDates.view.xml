<?xml version="1.0" encoding="UTF-8"?>
<View xmlns="http://www.butent.com/view"

  name="ServiceDates" source="ServiceDates"
  caption="=svcDates" newRowCaption="=svcNewDate" editForm="ServiceDate" newRowForm="ServiceDate"
  >
  <Columns>
    <SimpleColumn name="ServiceObject" />

    <SimpleColumn name="DateFrom" />
    <SimpleColumn name="DateUntil" />
    
    <SimpleColumn name="Color" />
    <SimpleColumn name="Note" />

    <SimpleColumn name="ServiceMaintenance" />

    <SimpleColumn name="Run" />
    <SimpleColumn name="TradeActService" />
    <SimpleJoin name="TradeActService" joinType="LEFT">
      <SimpleColumn name="TradeAct" />
      <SimpleJoin name="TradeAct" joinType="LEFT">
        <SimpleJoin joinType="LEFT" name="Series">
          <SimpleColumn name="SeriesName" alias="TradeSeriesName" />
        </SimpleJoin>
        <SimpleColumn name="Number" alias="TradeNumber" />
      </SimpleJoin>
    </SimpleJoin>
  </Columns>
  <Order>
    <OrderBy column="DateFrom" />
  </Order>
</View>
 