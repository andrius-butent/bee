<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<BeeTables xmlns="http://www.butent.com/bee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.butent.com/bee ../schemas/structure.xsd">

  <BeeTable name="DataTypes">
    <BeeFields>
      <BeeField name="Boolean" type="BOOLEAN" />
      <BeeField name="Integer" type="INTEGER" />
      <BeeField name="Long" type="LONG" />
      <BeeField name="Double" type="DOUBLE" />
      <BeeField name="Numeric" type="DECIMAL" precision="10" scale="5" />
      <BeeField name="Currency" type="DECIMAL" precision="12" scale="2" />
      <BeeField name="Char" type="CHAR" precision="10" />
      <BeeField name="String" type="STRING" precision="30" />
      <BeeField name="Memo" type="STRING" precision="400" />
      <BeeField name="Date" type="DATE" />
      <BeeField name="DateTime" type="DATETIME" />
    </BeeFields>
  </BeeTable>
</BeeTables>
